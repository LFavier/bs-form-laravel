{
    "name": "regnilk/bs-form-laravel",
    "description": "Laravel component for LaravelCollective forms (using Bootstrap)",
    "type": "library",
    "keywords": ["laravel", "font-awesome", "icon"],
    "license": "MIT",
    "homepage": "https://github.com/regnilk/bs-form-laravel",
    "authors": [
        {
            "name": "Regnilk",
            "email": "jack@regnilk.fr"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/config": "^7.0|^8.0",
        "illuminate/support": "^7.0|^8.0",
        "regnilk/fa-laravel": "^1.1",
        "laravelcollective/html": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Regnilk\\BsFormLaravel\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
              "Regnilk\\BsFormLaravel\\BsFormServiceProvider"
            ]
        }
    }
}
